{
  "name": "@dev-wizard/ui-terminal",
  "version": "0.0.4",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts --max-warnings=0",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc -p tsconfig.test.json"
  },
  "dependencies": {
    "@clack/core": "^0.5.0",
    "@clack/prompts": "^0.11.0",
    "@dev-wizard/engine": "workspace:*",
    "chalk": "^5.6.2",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
